# Copy this file to .env and fill in your API keys for Docker development

# ElevenLabs TTS API (required for audio generation)
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here

# Anthropic Claude API (required for script generation)  
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Replicate API (required for cover image generation)
REPLICATE_API_TOKEN=your_replicate_token_here

# Trello API (optional - for waitlist management)
TRELLO_API_KEY=your_trello_api_key_here
TRELLO_TOKEN=your_trello_token_here

# Database connection (automatically configured for Docker)
# DATABASE_URL=postgres://postgres:postgres@db:5432/fablino